<?xml version="1.0"?>
<JPanel xmlns:layout="https://github.com/daanipuui/swing-inflater/swing-inflater.xsd">

    <CardLayout/>

    <JPanel
        name="first"
        layout:cardId="FIRST">

        <JLabel text="This is on first card"/>

    </JPanel>

    <JPanel
        name="second"
        layout:cardId="SECOND">

        <JLabel text="This is on second card"/>

    </JPanel>

    <JPanel
        name="third"
        layout:cardId="THIRD">

        <JLabel text="This is on third card"/>

        <JTree
            name="file-tree"
            selectionRows="1, 2, 3"/>

    </JPanel>

</JPanel>